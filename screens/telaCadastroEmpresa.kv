<TelaCadastroEmpresa>
    BoxLayout:
        orientation: 'vertical'
        padding: 50
        canvas.before:
            Color:
                rgba: (43/255, 55/255, 63/255, 1)
            Rectangle:
                pos: self.pos
                size: self.size
        Label:
            text: 'Nova Empresa'
            font_size: '25sp'
            text_size: (self.width, None)
            size_hint: (1, 0.1)
        BoxRounded:
            orientation: 'vertical'
            pos_hint: {'center_x': 0.5, 'center_y': 0.5}
            padding: 20
            Label:
                text: 'Dados cadastrais'
                font_size: '16sp'
                text_size: (self.width, None)
                size_hint: (1, 0.1)
            LinhaDados:
                ColunaDado:
                    size_hint_x: 0.3
                    id: idCnpj
                    TextoForm:
                        text: 'CNPJ'
                ColunaDado:
                    TextoForm:
                        text: 'Razão social'
                    TextInput:
                        id: razaoSocial
            LinhaDados:
                ColunaDado:
                    TextoForm:
                        text: 'Fantasia'
                    TextInput:
                        id: fantasia
                ColunaDado:
                    TextoForm:
                        text: 'Telefone'
                    TextInput:
                        id: telefone
                ColunaDado:
                    TextoForm:
                        text: 'E-mail'
                    TextInput:
                        id: email
            LinhaDados:
                ColunaDado:
                    TextoForm:
                        text: 'CEP'
                    TextInput:
                        id: cep
                ColunaDado:
                    TextoForm:
                        text: 'Logradouro'
                    TextInput:
                        id: logradouro
                ColunaDado:
                    TextoForm:
                        text: 'Bairro'
                    TextInput:
                        id: bairro
                ColunaDado:
                    TextoForm:
                        text: 'Número'
                    TextInput:
                        id: numero
                ColunaDado:
                    TextoForm:
                        text: 'Cidade'
                    TextInput:
                        id: cidade
            LinhaDados:
                ColunaDado:
                    size_hint_x: 0.3
                    TextoForm:
                        text: 'Tipo'
                    TextInput:
                        id: tipo
                ColunaDado:
                    TextoForm:
                        text: 'Matriz'
                    TextInput:
                        id: matriz
            RoundedButton:
                text: 'Cadastrar'
                on_press: root.cadastrarDados()