<BarraLateral>
    orientation: 'vertical'
    size_hint_x: 0.2
    canvas.before:
        Color:
            rgba: (23/255, 31/255, 36/255, 1)
        Rectangle:
            pos: self.pos
            size: self.size
    BoxLayout:
        orientation: 'vertical'
        size_hint_y: None
        height: 400
        BoxLayout:
            orientation: 'vertical'
            size_hint_y: None
            height: 50
            padding: 30, 10
            RoundedButton:
                size_hint: (1, 1)
                text: 'Solicitações'
        BoxLayout:
            orientation: 'vertical'
            size_hint_y: None
            height: 170
            BoxLayout:
                orientation: 'vertical'
                size_hint_y: None
                height: 50
                padding: 30, 10
                RoundedButton:
                    size_hint: (1, 1)
                    text: 'Usuários'
            BoxLayout:
                orientation: 'vertical'
                size_hint_y: None
                height: 50
                padding: 30, 10
                Button:
                    text: 'Novo usuário'
                    on_press: root.mudaTela("TelaCadastroUsuario")
            BoxLayout:
                orientation: 'vertical'
                size_hint_y: None
                height: 50
                padding: 30, 10
                Button:
                    text: 'Editar usuário'
                    on_press: root.mudaTela("TelaUsuarios")
        BoxLayout:
            orientation: 'vertical'
            size_hint_y: None
            height: 170
            BoxLayout:
                orientation: 'vertical'
                size_hint_y: None
                height: 50
                padding: 30, 10
                RoundedButton:
                    size_hint: (1, 1)
                    text: 'Empresa'
            BoxLayout:
                orientation: 'vertical'
                size_hint_y: None
                height: 50
                padding: 30, 10
                Button:
                    text: 'Nova empresa'
                    on_press: root.mudaTela("TelaCadastroEmpresa")
            BoxLayout:
                orientation: 'vertical'
                size_hint_y: None
                height: 50
                padding: 30, 10
                Button:
                    text: 'Editar empresa'
    BoxLayout:
        orientation: "vertical"